<header class="text-gray-600 body-font">
    <div class="container mx-auto flex flex-wrap p-5 flex-col md:flex-row items-center">
      <a href="/" class="flex title-font font-medium items-center text-gray-900 mb-4 md:mb-0">
        <%- include('./svg/brand')%>
        <span class="text-xl text-yellow-500"><%= process.env.title %></span>
      </a>
      <nav class="md:mr-auto md:ml-4 md:py-1 md:pl-4 md:border-l md:border-gray-400	flex flex-wrap items-center text-base justify-center">
        <a href="/author" class="mr-5 hover:text-gray-900 <% if(typeof author !== 'undefined'){ %> bg-indigo-200 rounded-xl p-1<% } %>"><%- include('./svg/author') %></a>
        <a href="/quote" class="mr-5 hover:text-gray-900 <% if(typeof quote !== 'undefined'){ %> bg-indigo-200 rounded-xl p-1<% } %>"><%- include('./svg/quote') %></a>
        <a href="/category" class="mr-5 hover:text-gray-900 <% if(typeof category !== 'undefined'){ %> bg-indigo-200 rounded-xl p-1<% } %>"><%- include('./svg/category') %></a>
        <form action="/auth/logout" class="" method="POST">
          <button class="hover:text-gray-900" type="submit">
            <%- include('./svg/logout') %>
          </button>
        </form>
      </nav>
    </div>
  </header>
  <span class="inline-block h-1 w-full rounded bg-indigo-900"></span>

